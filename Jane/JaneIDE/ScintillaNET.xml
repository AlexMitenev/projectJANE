<?xml version="1.0" encoding="utf-8"?>

<ScintillaNET>
    <Language Name="jane">

        <Lexer LexerName="cpp"/>
        <Lexer LineCommentPrefix="//" StreamCommentPrefix="/*" StreamCommentSuffix="*/" >
            <Keywords List="0">class yes public static import new interface main args</Keywords>
            <Keywords List="1">void int string String</Keywords>
        </Lexer>
        <Styles>
        	<Style Name="DEFAULT" FontName="Courier New" Size="10"/>
            <Style Name="NUMBER" ForeColor="#00E639" Bold="Yes"/>
            <Style Name="COMMENT" ForeColor="Green"/>
            <Style Name="STRING" ForeColor="Red" />
            <Style Name="WORD" ForeColor="#CA1454" />
            <Style Name="WORD2" ForeColor="#00B3FF"/>
            <Style Name="OPERATOR" ForeColor="#FF531A"/>
        </Styles>

        <AutoComplete FillUpCharacters="" SingleLineAccept="True" IsCaseSensitive="False">
        <List>
        	boolean break catch class const declare delay do else error exit false float for gosub goto if in int long null retry return string suspend throw true try var void while 
        </List>
        </AutoComplete>	

        <Indentation TabWidth="4" UseTabs="true"/>

    </Language>

</ScintillaNET>